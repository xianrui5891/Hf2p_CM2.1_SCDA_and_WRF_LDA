!!$       corr_t(j_ens)%data(isc:iec,jsc:jec,1:nk) =&
!!$            & temp_ens_tau(j_ens)%data(isc:iec,jsc:jec,1:nk) - corr_t(j_ens)%data(isc:iec,jsc:jec,1:nk)
!!$       corr_s(j_ens)%data(isc:iec,jsc:jec,1:nk) =&
!!$            & salt_ens_tau(j_ens)%data(isc:iec,jsc:jec,1:nk) - corr_s(j_ens)%data(isc:iec,jsc:jec,1:nk)
!!$    end do
!!$    ! extend the adj amount to the deep water
