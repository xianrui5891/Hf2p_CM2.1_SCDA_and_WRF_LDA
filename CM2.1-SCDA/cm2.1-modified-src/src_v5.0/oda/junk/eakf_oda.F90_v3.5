    call obs_end()

    first_run_call = .false.

    call mpp_clock_end(id_eakf_total)

    if ( debug_eakf ) then
       write (UNIT=stdout_unit, FMT='("PE ",I5,": finished ensemble_filter")') mpp_pe()
    end if

  end subroutine ensemble_filter


